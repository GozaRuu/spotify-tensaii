{
  "name": "create-next-example-app",
  "version": "0.1.0",
  "description": "Fullstack Spotify API consumer app",
  "author": "Kais Sghari",
  "homepage": "https://github.com/GozaRuu/spotify-tensaii#readme",
  "license": "MIT",
  "main": "server/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GozaRuu/spotify-tensaii.git"
  },
  "keywords": [
    "Node",
    "Postgres",
    "Knex",
    "Express",
    "GraphQL",
    "Apollo",
    "Next",
    "React"
  ],
  "scripts": {
    "migrate": "knex migrate:latest --cwd ./database",
    "rollback": "knex migrate:rollback --cwd ./database",
    "seed": "yarn rollback && yarn migrate && knex seed:run --cwd ./database",
    "dev": "yarn migrate && nodemon server",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "test": "jest --watch"
  },
  "dependencies": {
    "apollo-boost": "^0.1.23",
    "apollo-datasource": "^0.2.1",
    "apollo-datasource-rest": "^0.2.1",
    "apollo-server-express": "^2.3.1",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "jest": "^23.6.0",
    "jsonwebtoken": "^8.4.0",
    "knex": "^0.16.2",
    "next": "^7.0.2",
    "objection": "^1.4.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.7.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "bugs": {
    "url": "https://github.com/GozaRuu/spotify-tensaii/issues"
  },
  "devDependencies": {
    "morgan": "^1.9.1",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "sqlite3": "^4.0.4"
  }
}
